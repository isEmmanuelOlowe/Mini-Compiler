
	PROC UNI ( LEVEL )
	    IF LEVEL == 0 THEN
		FORWARD 0
	    ELSE
		FORWARD LEVEL
		RIGHT 185
		LUNI ( LEVEL - 1 )
	    ENDIF 

	PROC LUNI ( LEVEL )
	    IF LEVEL == 0 THEN
		FORWARD 0
	    ELSE
		FORWARD LEVEL
		LEFT 195
		RUNI ( LEVEL - 1 )
	    ENDIF 
	
	PROC RUNI ( LEVEL )
	    IF LEVEL == 0 THEN
		FORWARD 0
	    ELSE
		FORWARD LEVEL
		LEFT 60
		UNI ( LEVEL - 1 )
	    ENDIF 

	PROC MAIN (VOID)
	   UNI ( 140 )