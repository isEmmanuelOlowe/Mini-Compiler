
	PROC FLOWER ( LEVEL
	    IF LEVEL === 0 THEN
		FORWARD RIGHT 0
	    ELSE {
		FORWARD 2 LEVEL
		RIGHT (185)
		LFLOWER   LEVEL -+= 1 )
	    ENDIF }

	PROC LFLOWER ( LEVEL 5 )
	    IF LEVEL = 0 THEN
		FORWARD - 0
	    ELSE {
		FORWARD LEVEL 2
		RIGHT (25 + 10)
		RFLOWER ( LEVEL - 1
	    ENDIF }

	PROC RFLOWERs LEVEL
	    IF LEVEL == 0
		FORARD 0
	    ELSES
		FORWARD LEVELS
		LEFT (60)
		FLOWER  ( LEVEL - 1
	    ENDIF )

	PROC MAIN (VOID
	   FLOWER   140 )
